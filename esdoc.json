{
    "source": "./src",
    "destination": "./doc",
    "plugins": [
        {"name": "esdoc-standard-plugin"},
        {
            "name": "esdoc-ecmascript-proposal-plugin",
            "option": {
                "classProperties": true,
                "objectRestSpread": true,
                "doExpressions": true,
                "functionBind": true,
                "functionSent": true,
                "asyncGenerators": true,
                "decorators": true,
                "exportExtensions": true,
                "dynamicImport": true
            }
        },
        {
            "name": "esdoc-brand-plugin",
            "option": {
                "title": "Starting Blocks JS framework",
                "description": "A page transition and blocks ES6 framework by REZO ZERO",
                "repository": "https://github.com/rezozero/starting-blocks",
                "site": "https://startingblocks.rezo-zero.com/",
                "author": "https://www.rezo-zero.com/"
            }
        }
    ]
}
